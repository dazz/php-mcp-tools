{
    "name": "dazz/php-mcp-tools",
    "description": "fill description",
    "type": "library",
    "license": "MIT",
    "keywords": ["mcp", "tool", "llm"],
    "authors": [
        {
            "name": "dazz",
            "email": "dazz@c-base.org"
        }
    ],
    "require": {
        "php": ">=8.2",
        "php-llm/mcp-sdk": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "phpstan/phpstan": "^2.1",
        "rector/rector": "^2.0",
        "phpstan/phpstan-webmozart-assert": "^2.0",
        "php-cs-fixer/shim": "^3.9"
    },
    "autoload": {
        "psr-4": {
            "Dazz\\PhpMcpTools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dazz\\PhpMcpTools\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
